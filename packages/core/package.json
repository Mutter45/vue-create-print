{
  "name": "vue-create-print",
  "version": "0.0.3",
  "description": "web print for vue2.x and vue3.x",
  "author": "Mutter45 <mutter45@foxmail.com>",
  "license": "ISC",
  "homepage": "https://github.com/Mutter45/vue-create-print#README",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mutter45/vue-create-print.git"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "keywords": [
    "print",
    "vue",
    "vue-create-print"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "default": "./dist/index.umd.js"
    }
  },
  "main": "index.js",
  "unpkg": "./dist/index.iife.js",
  "jsdelivr": "./dist/index.iife.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "cross-env NODE_ENV=development vite build --watch",
    "build": "cross-env NODE_ENV=production vite build",
    "release": "release-it"
  },
  "release-it": {
    "git": false,
    "npm": {
      "publish": true
    }
  },
  "peerDependenciesMeta": {
    "@vue/composition-api": {
      "optional": true
    }
  },
  "dependencies": {
    "lodash-es": "^4.17.21",
    "vue-demi": "^0.14.8"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.12"
  }
}
